/**
 * 16-bit dmux/zero/mux for use in ALU
 */

CHIP ALUNegZero {
    IN in[16], n, z;
    OUT out[16];

    PARTS:
    DMux16(in=in, sel=z, a=znoop, b=zphase1);
    Not16(in=zphase1, out=zphase2);
    And16(a=znoop, b=zphase2, out=zphase3);
    Mux16(a=znoop, b=zphase3, sel=z, out=trx1);

    DMux16(in=trx1, sel=n, a=nnoop, b=nphase1);
    Not16(in=nphase1, out=nphase2);
    Mux16(a=nnoop, b=nphase2, sel=n, out=out);

}
